<%= layout("layouts/boilerplate") %>
<div>
  <h3>User Details</h3>
  <div>
    <h3>username: <i><%= user.username %></i></h3>
  </div>
  <div class="btns">
    <a href="/members/<%= user._id %>/edit" class="btn btn-dark">Edit</a
    ><br /><br />
    <form method="post" action="/members/<%= user._id %>?_method=DELETE">
      <button class="btn btn-danger">Delete</button>
    </form>
  </div>
</div>
